{"ast":null,"code":"var body = document.getElementsByTagName('body')[0];\nimport { mapState } from 'vuex';\nexport default {\n  name: 'SearchBar',\n  data() {\n    return {\n      terms: '',\n      searchResult: false,\n      searchResultEmpty: false\n    };\n  },\n  computed: {\n    ...mapState({\n      menuItems: state => state.menu.searchData,\n      searchOpen: state => state.menu.searchOpen\n    })\n  },\n  watch: {\n    terms: {\n      handler(value) {\n        this.terms ? this.addFix() : this.removeFix();\n        this.$store.dispatch('menu/searchTerm', value);\n        if (!this.menuItems.length) this.searchResultEmpty = true;else this.searchResultEmpty = false;\n      },\n      deep: true\n    }\n  },\n  methods: {\n    search_open() {\n      this.$store.state.menu.searchOpen = true;\n    },\n    search_close() {\n      this.$store.state.menu.searchOpen = false;\n      body.classList.remove('offcanvas');\n    },\n    searchterm() {\n      this.$store.dispatch('menu/searchTerm', this.terms);\n    },\n    addFix() {\n      body.classList.add('offcanvas');\n      this.searchResult = true;\n    },\n    removeFix() {\n      body.classList.remove('offcanvas');\n      this.searchResult = false;\n      this.terms = '';\n    }\n  }\n};","map":{"version":3,"names":["body","document","getElementsByTagName","mapState","name","data","terms","searchResult","searchResultEmpty","computed","menuItems","state","menu","searchData","searchOpen","watch","handler","value","addFix","removeFix","$store","dispatch","length","deep","methods","search_open","search_close","classList","remove","searchterm","add"],"sources":["C:\\Users\\chukwuebuka.ohaji\\Workspace\\Tests\\HomeCareAgency\\web\\src\\components\\header\\search.vue"],"sourcesContent":["<template>\n  <form\n    class=\"form-inline search-full col\"\n    action=\"#\"\n    method=\"get\"\n    @submit.prevent=\"\"\n    :class=\"searchOpen?'open':'' \"\n  >\n    <div class=\"form-group w-100\">\n      <div class=\"Typeahead Typeahead--twitterUsers\">\n        <div class=\"u-posRelative\">\n          <input\n            class=\"demo-input Typeahead-input form-control-plaintext w-100\"\n            type=\"text\"\n            v-model=\"terms\" \n            placeholder=\"Search Cuba ..\"\n            name=\"q\"\n            title=\"\"\n            autofocus\n            \n          />\n          <div class=\"spinner-border Typeahead-spinner\" role=\"status\">\n            <span class=\"sr-only\">Loading...</span>\n          </div>\n          <vue-feather\n            class=\"close-search\"\n            type=\"x\"\n            @click=\"search_close()\"\n          ></vue-feather>\n        </div>\n        <div\n          :class=\"searchResult ? 'Typeahead-menu is-open' : 'Typeahead-menu'\"\n          v-if=\"menuItems.length\"\n        >\n          <div\n            class=\"ProfileCard u-cf\"\n            v-for=\"(menuItem, index) in menuItems.slice(0, 8)\"\n            :key=\"index\"\n          >\n            <div class=\"ProfileCard-avatar header-search\">\n              <!-- <vue-feather :type=\"menuItem.icon\"></vue-feather> -->\n              <svg class=\"stroke-icon\">\n                <use :xlink:href=\"require('@/assets/svg/icon-sprite.svg')+`#${menuItem.icon}`\"></use>\n              </svg>\n            </div>\n            <div class=\"ProfileCard-details\">\n              <div class=\"ProfileCard-realName\">\n                <span @click=\"removeFix()\"\n                  ><router-link\n                    :to=\"{ path: menuItem.path }\"\n                    class=\"realname\"\n                    >{{ menuItem.title }}</router-link\n                  ></span\n                >\n              </div>\n            </div>\n          </div>\n        </div>\n        <div\n          :class=\"\n          searchResultEmpty ? 'Typeahead-menu is-open' : 'Typeahead-menu'\n          \"\n        >\n          <div class=\"tt-dataset tt-dataset-0\">\n            <div class=\"EmptyMessage\">\n              Your search turned up 0 results. Opps There are no result found.\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </form>\n</template>\n\n<script>\n  var body = document.getElementsByTagName('body')[0];\n  import { mapState } from 'vuex';\n\n  export default {\n    name: 'SearchBar',\n    data () {\n      return {\n        terms: '',\n        searchResult: false,\n        searchResultEmpty: false,\n      };\n    },\n    computed: {\n      ...mapState({\n        menuItems: (state) => state.menu.searchData,\n        searchOpen: (state) => state.menu.searchOpen\n      }),\n    },\n    watch: {\n    terms:{\n      handler(value){\n          this.terms ? this.addFix() : this.removeFix();\n        this.$store.dispatch('menu/searchTerm', value);\n        if (!this.menuItems.length) this.searchResultEmpty = true;\n        else this.searchResultEmpty = false;\n      },deep:true\n    }\n    },\n    methods: {\n      search_open() {\n        this.$store.state.menu.searchOpen = true;\n      },\n      search_close() {\n        this.$store.state.menu.searchOpen = false;\n        body.classList.remove('offcanvas');\n      },\n      searchterm(){\n        this.$store.dispatch('menu/searchTerm', this.terms);\n      },\n      addFix() {\n        body.classList.add('offcanvas');\n        this.searchResult = true;\n      },\n      removeFix() {\n        body.classList.remove('offcanvas');\n        this.searchResult = false;\n        this.terms = '';\n      },\n    },\n  };\n</script>\n"],"mappings":"AA2EE,IAAIA,IAAG,GAAIC,QAAQ,CAACC,oBAAoB,CAAC,MAAM,CAAC,CAAC,CAAC,CAAC;AACnD,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,KAAK;MACnBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGN,QAAQ,CAAC;MACVO,SAAS,EAAGC,KAAK,IAAKA,KAAK,CAACC,IAAI,CAACC,UAAU;MAC3CC,UAAU,EAAGH,KAAK,IAAKA,KAAK,CAACC,IAAI,CAACE;IACpC,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACPT,KAAK,EAAC;MACJU,OAAOA,CA