\\\" :class=\\\"{'horizontal-wrapper':sidebar=='horizontal-wrapper','compact-wrapper':sidebar=='compact-sidebar1'}\\\"> \"), _createElementVNode(\"div\", {\n    class: _normalizeClass([\"page-wrapper\", $data.layoutobj]),\n    id: \"pageWrapper\"\n  }, [_createElementVNode(\"div\", {\n    class: _normalizeClass([\"page-header\", {\n      close_icon: !_ctx.togglesidebar\n    }])\n  }, [_createVNode(_component_Header, {\n    onClicked: $options.sidebar_toggle\n  }, null, 8 /* PROPS */, [\"onClicked\"])], 2 /* CLASS */), _createElementVNode(\"div\", _hoisted_6, [_createCommentVNode(\" <div class=\\\"bg-overlay\\\" :class=\\\"{active: activeoverlay }\\\" @click=\\\"removeoverlay()\\\"></div> \"), _createElementVNode(\"div\", {\n    class: _normalizeClass([\"sidebar-wrapper\", [{\n      close_icon: !_ctx.togglesidebar\n    }]]),\n    \"sidebar-layout\": _ctx.svg == 'stroke-svg' ? 'stroke-svg' : 'fill-svg'\n  }, [_createVNode(_component_Sidebar, {\n    onClicked: $options.sidebar_toggle\n  }, null, 8 /* PROPS */, [\"onClicked\"])], 10 /* CLASS, PROPS */, _hoisted_7), _createElementVNode(\"div\", {\n    class: \"page-body\",\n    onClick: _cache[0] || (_cache[0] = $event => $options.hidesecondmenu())\n  }, [_createVNode(_Transition, {\n    name: \"fadeIn\",\n    \"enter-active-class\": \"animated fadeIn\"\n  }, {\n    default: _withCtx(() => [_createVNode(_component_router_view, {\n      class: \"view\"\n    })]),\n    _: 1 /* STABLE */\n  })]), _createVNode(_component_Footer)]), _createCommentVNode(\" <Customizer/> \"), _createVNode(_component_TapTop)], 2 /* CLASS */)]));\n}","map":{"version":3,"names":["key","class","_createElementVNode","id","$data","loading","_createElementBlock","_hoisted_1","_hoisted_2","_hoisted_3","_hoisted_4","_createVNode","_component_fegaussianblur","in","stddeviation","result","_component_fecolormatrix","values","_hoisted_5","_createCommentVNode","_normalizeClass","layoutobj","close_icon","_ctx","togglesidebar","_component_Header","onClicked","