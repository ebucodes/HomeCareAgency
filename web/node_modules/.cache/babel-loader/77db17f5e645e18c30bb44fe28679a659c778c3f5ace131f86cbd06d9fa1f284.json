{"ast":null,"code":"import formValidation from \"@/mixins/commen/formValidation\";\nexport default {\n  mixins: [formValidation],\n  data() {\n    return {\n      active: true\n    };\n  },\n  methods: {\n    show() {\n      this.active = !this.active;\n    }\n  }\n};","map":{"version":3,"names":["formValidation","mixins","data","active","methods","show"],"sources":["C:\\Users\\chukwuebuka.ohaji\\Workspace\\Tests\\HomeCareAgency\\web\\src\\pages\\auth\\login.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n            <div class=\"col-xl-7 order-1 b-center bg-size\"\r\n                :style=\"{ backgroundImage: 'url(' + require('../../assets/images/login/1.jpg') + ')' }\"\r\n                style=\"background-size: cover; background-position: center center; display: block;\">\r\n                <img class=\"bg-img-cover bg-center\" src=\"../../assets/images/login/1.jpg\" alt=\"looginpage\"\r\n                    style=\"display: none;\" />\r\n            </div>\r\n            <div class=\"col-xl-5 p-0\">\r\n                <div class=\"login-card\">\r\n                    <div>\r\n                        <div>\r\n                            <a class=\"logo text-start\">\r\n                                <img class=\"img-fluid for-light\" src=\"../../assets/images/logo/logo.png\"\r\n                                    alt=\"looginpage\" />\r\n                                <img class=\"img-fluid for-dark\" src=\"../../assets/images/logo/logo_dark.png\"\r\n                                    alt=\"looginpage\" /></a>\r\n                        </div>\r\n                        <div class=\"login-main\">\r\n                            <form class=\"needs-validation\" novalidate=\"\" @submit.prevent=\"onCustomStyleSubmit\">\r\n                                <h4>Sign in to account</h4>\r\n                                <p>Enter your email & password to login</p>\r\n                                <div class=\"form-group\">\r\n                                    <label class=\"col-form-label\">Username</label>\r\n                                    <input type=\"text\" class=\"form-control\"\r\n                                        v-bind:class=\"formSubmitted ? firstError ? 'is-invalid' : 'is-valid' : ''\"\r\n                                        id=\"name\" placeholder=\"Enter your username\" v-model=\"userName\">\r\n                                    <div class=\"valid-feedback\" id=\"feedback-1\" v-if=\"errors[0]\">{{ errors[0].message }}\r\n                                    </div>\r\n\r\n                                </div>\r\n                                <div class=\"form-group\">\r\n                                    <label class=\"col-form-label\">Password</label>\r\n                                    <div class=\"form-input position-relative\">\r\n                                        <input class=\"form-control\" :type=\"active ? 'password' : 'text'\"\r\n                                            name=\"login[password]\" placeholder=\"*********\" required>\r\n                                        <div class=\"show-hide\">\r\n                                            <span class=\"show\" :class=\"active ? 'show' : 'hide'\" @click.prevent=\"show\">\r\n                                            </span>\r\n                                        </div>\r\n                                    </div>\r\n                                </div>\r\n                                <div class=\"form-group mb-0\">\r\n                                    <div class=\"checkbox p-0\">\r\n                                        <input id=\"checkbox1\" type=\"checkbox\">\r\n                                        <label class=\"text-muted\" for=\"checkbox1\">Remember password</label>\r\n                                    </div>\r\n                                    <router-link to=\"/\" class=\"link\">Forgot password?</router-link>\r\n                                    <div class=\"text-end mt-3\">\r\n                                        <router-link to=\"/\">\r\n                                            <button class=\"btn btn-primary btn-block w-100\" type=\"submit\">Sign\r\n                                                in</button></router-link>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <p class=\"mt-4 mb-0 text-center\">Don't have account?\r\n                                    <router-link class=\"ms-2\" to=\"/auth/register\">Create Account</router-link>\r\n                                </p>\r\n\r\n                            </form>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport formValidation from \"@/mixins/commen/formValidation\"\r\nexport default {\r\n    mixins: [formValidation],\r\n    data() {\r\n        return {\r\n            active: true\r\n        }\r\n    },\r\n    methods: {\r\n        show() {\r\n            this.active = !this.active\r\n        }\r\n    }\r\n}\r\n</script>\r\n"],"mappings":"AAsEA,OAAOA,cAAa,MAAO,gCAA+B;AAC1D,eAAe;EACXC,MAAM,EAAE,CAACD,cAAc,CAAC;EACxBE,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,IAAIA,CAAA,EAAG;MACH,IAAI,CAACF,MAAK,GAAI,CAAC,IAAI,CAACA,MAAK;IAC7B;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}